<p id="notice"><%= notice %></p>
<% provide(:title, "Monk Shugenja Attachments") %>

<div class="container-fluid">
  <% provide(:panel_title, "Monk Shugenja Attachments") %>
  <table class="table table-striped table-bordered">
    <thead>
    <tr>
      <th>Title</th>
      <th>Type</th>
      <th>Gold Cost</th>
      <th>Keywords</th>
      <th>Description</th>
      <th>Focus</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @monk_shugenja_attachments.each do |monk_shugenja_attachment| %>
      <tr>
        <td><%= monk_shugenja_attachment.title %></td>
        <td><%= type_title(monk_shugenja_attachment.typeID)%></td>
        <td><%= monk_shugenja_attachment.goldCost %></td>
        <td><%= monk_shugenja_attachment.keywords %></td>
        <td><%= monk_shugenja_attachment.description %></td>
        <td><%= monk_shugenja_attachment.focus %></td>
        <td><%= link_to 'Show', monk_shugenja_attachment %></td>

      </tr>
    <% end %>
  </tbody>
</table>
</div>