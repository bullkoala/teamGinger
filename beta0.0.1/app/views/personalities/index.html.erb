<p id="notice"><%= notice %></p>

<h1>Listing Personalities</h1>



<%= form_tag(personalities_path, :method => "get", id: "search-form") do %>
      <%= text_field_tag :search, params[:search], placeholder: "Search Personalities" %>
      <%= submit_tag "Search", :name => nil %>
  <% end %>
<br>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Typeid</th>
      <th>Strength</th>
      <th>Chi</th>
      <th>Goldcost</th>
      <th>Familyhonorrequirement</th>
      <th>Personalhonor</th>
      <th>Keywords</th>
      <th>Description</th>
      <th>Clan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @personalities.each do |personality| %>
      <tr>
        <td><%= personality.title %></td>
        <td><%= personality.typeID %></td>
        <td><%= personality.strength %></td>
        <td><%= personality.chi %></td>
        <td><%= personality.goldCost %></td>
        <td><%= personality.familyHonorRequirement %></td>
        <td><%= personality.personalHonor %></td>
        <td><%= personality.keywords %></td>
        <td><%= personality.description %></td>
        <td><%= personality.clan %></td>
        <td><%= link_to 'Show', personality %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>
