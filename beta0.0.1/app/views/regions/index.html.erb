<p id="notice"><%= notice %></p>
<% provide(:title, "Region") %>

<div class="container-fluid">
  <% provide(:panel_title, "Region") %>
  <table class="table table-striped table-bordered">

  <thead>
    <tr>
      <th>Title</th>
      <th>Type</th>
      <th>Keywords</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @regions.each do |region| %>
      <tr>
        <td><%= region.title %></td>
        <td><%= type_title(region.typeID)%></td>
        <td><%= region.keywords %></td>
        <td><%= region.description %></td>
        <td><%= link_to 'Show', region %></td>

      </tr>
    <% end %>
  </tbody>
</table></div>
