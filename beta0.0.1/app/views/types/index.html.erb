<p id="notice"><%= notice %></p>

<h1>Listing Types</h1>



<%= form_tag(types_path, :method => "get", id: "search-form") do %>
      <%= text_field_tag :search, params[:search], placeholder: "Search Types" %>
      <%= submit_tag "Search", :name => nil %>
  <% end %>
<br>
<table>
  <thead>
    <tr>
      <th>Typename</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @types.each do |type| %>
      <tr>
        <td><%= type.typeName %></td>
        <td><%= link_to 'Show', type %></td>

      </tr>
    <% end %>
  </tbody>
</table>



<br>
