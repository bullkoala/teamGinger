<p id="notice"><%= notice %></p>
<% provide(:title, "Types") %>

<div class="container-fluid">
  <hr>
  <%= render 'search' %>
  <hr>
  <% provide(:panel_title, "Types") %>
  <table class="table table-striped table-bordered">

  <thead>
    <tr>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @types.each do |type| %>
      <tr>
        <td><%= type.typeName %></td>
        <td><%= link_to 'Show', type %></td>

      </tr>
    <% end %>
  </tbody>
</table>
</div>